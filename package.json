{
  "name": "yyl-hander",
  "version": "0.12.2",
  "description": "",
  "main": "lib/yh.js",
  "types": "lib/yh.d.ts",
  "eslintConfig": {
    "root": true,
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-node"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "test/case",
    "test/__frag"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "trailingComma": "none"
  },
  "scripts": {
    "test": "jest ./test/jest --env=node",
    "test:formatConfig": "jest ./test/jest/formatConfig.test.js --env=node",
    "test:hideProtocol": "jest ./test/jest/hideProtocol.test.js --env=node",
    "test:optimize.getHomePage": "jest ./test/jest/optimize.getHomePage.test.js --env=node",
    "test:optimize.initPlugins": "jest ./test/jest/optimize.initPlugins.test.js --env=node",
    "test:optimize.initScripts": "jest ./test/jest/optimize.initScripts.test.js --env=node",
    "test:parseConfig": "jest ./test/jest/parseConfig.test.js --env=node",
    "test:setVar": "jest ./test/jest/setVar.test.js --env=node",
    "test:sugarReplace": "jest ./test/jest/sugarReplace.test.js --env=node",
    "eslint": "eslint ./",
    "prettier": "prettier --write ./",
    "prepublishOnly": "npm run prettier & npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-hander.git"
  },
  "keywords": [
    "yyl",
    "handler"
  ],
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-hander/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-hander#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "concat-with-sourcemaps": "^1.1.0",
    "rev-hash": "^3.0.0",
    "yyl-config-types": "^0.2.0",
    "yyl-os": "0.11.0",
    "yyl-request": "^0.1.1",
    "yyl-util": "^2.3.8"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-yyl-node": "^0.1.2",
    "jest": "^26.5.2",
    "prettier": "^2.0.4",
    "yyl-fs": "^0.3.2",
    "yyl-print": "^0.7.3"
  }
}
